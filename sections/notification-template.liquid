  function checkproductpreorderstatus(){    
    var getproductenddate=$("#product_enddate").val();   
    if(getproductenddate!=undefined){
      var fullDate = new Date();

      //set dd/mm/yyyy format
      //convert month to 2 digits
      var twoDigitMonth = ((fullDate.getMonth().length+1) === 1)? (fullDate.getMonth()+1) : '0' + (fullDate.getMonth()+1);

      var currentDate = fullDate.getDate() + "/" + twoDigitMonth + "/" + fullDate.getFullYear();

      //call date function
      var dateObjProduct = getDateObj(getproductenddate);
      var dateObjCurrent = getDateObj(currentDate);


      //check date comparision
      if(dateObjProduct >= dateObjCurrent) {
        //console.log("if");
      }else{
        //console.log("else");
        $(".cart-btn").text("ADD TO CART");
        $(".pre_order_instruction").remove();
      }
    }
  }
  
      function getDateObj(dateString) {
        var parts = dateString.split('/');
        var date = parts[0];
        var month = parts[1];
        var year = parts[2];
        return new Date(year, month, date);
      }
</script>

