{% include 'breadcrumb' %}

{% assign in_hats_collection = false %}
{% for collection in product.collections %}
  {% if in_hats_collection == false and collection.title == 'Shoes' %}
    {% assign in_hats_collection = true %}

  {% endif %}
{% endfor %}

{% assign in_shipping_method = false %}
{% assign in_preorder_tag = false %}
{% assign collectionids = '' %}
{% assign coll_freeship = '' %}
{% assign coll_free_exp_ship = '' %}
{% assign ships_separatly = '' %}
{% assign three_ships_free = '' %}
{% assign freesheep = '' %}
{% assign free_exp_sheep = '' %}
{% assign drop_ships = '' %}
{% assign heavy_bulky = '' %}
{% assign free_ship_sticker = '' %}
{% assign free_expship_sticker = '' %}
{% assign coll_au_nz = '' %}
{% assign sticker_flag = 0 %}
{% assign cart_flag = 0 %}
{% assign isBulky = 0 %}
{% assign coll_freeship = collections[settings.free_shipping].id %}
{% assign coll_free_exp_ship = collections[settings.free_Express_shipping].id %}
{% assign ships_separatly = collections[settings.drop_ships].id %}
{% assign three_ships_free = collections[settings.three_ships_free].id %}
{% assign heavy_bulky = collections[settings.bulky_items].id %}
{% assign default_color = '' %}
{% for pcollection in product.collections %}
{% if pcollection.id == coll_freeship %}
{% assign freesheep = pcollection.id %}
{% endif %}
{% if pcollection.id == coll_free_exp_ship %}
{% assign free_exp_sheep = pcollection.id %}
{% endif %}
{% if pcollection.id == ships_separatly %}
{% assign drop_ships = pcollection.id %}
{% endif %}
{% if forloop.index == 1 %}
{% assign collectionids = pcollection.id %}
{% else %}
{% assign collectionids = collectionids | append:',' | append:pcollection.id %}
{% endif %}
{% endfor %}
{% assign prtag = '' %}
{% for tag in product.tags %}
{% if tag == 'countdown' or tag == 'Countdown'%}
{% assign prtag = tag %}
{% assign hide_shipping = '' %}
<span style="display:none;" class="pcol">{{product.type}}</span>
<section id="shipping-countdown">
  <div class="shipping-txt-bar">
    {% unless section.settings.logo_link == blank  %}<a href="{{ section.settings.logo_link }}">{% endunless %}
    {% if section.settings.logo_title == blank %}
    {% assign count = "2019/01/01" %}
    {% else %}
    {% assign count = section.settings.logo_title %}
    {% endif %}
    <div class="shipping-title">

      {{ settings.countdown_text}}

    </div>
    {% unless section.settings.logo_link == blank  %}</a>{% endunless %}
  </div>
</section>
{% endif %}
{% endfor %}
<!-- START FREE SHIPPING > $150 -->
{% assign heavy_bulky_coll = false %}
{% for block in section.blocks %}
{% if block.type == 'bulky_heavy_collection' %}
{% assign collectionh = block.settings.collection %}  
{% assign col_ids = collections[collectionh].id %}
{% for prod_id in product.collections %}       
{% if col_ids == prod_id.id %}
{% assign heavy_bulky_coll = false %}            
{% break %}
{% else %}
{% assign heavy_bulky_coll = true %}
{% endif %}
{% endfor %}
{% endif %}
{% endfor %}
<!-- END FREE SHIPPING > $150 -->

<div class="product-template__container page-width-large" itemscope itemtype="http://schema.org/Product" id="ProductSection-{{ section.id }}" data-section-id="{{ section.id }}" data-section-type="product" data-enable-history-state="true">
  <meta itemprop="url" content="{{ shop.url }}{{ product.url }}">
  <meta itemprop="image" content="{{ product.featured_image.src | img_url: '800x' }}">

  {% comment %}
  Get first variant, or deep linked one
  {% endcomment %}
  {%- assign current_variant = product.selected_or_first_available_variant -%}
  {%- assign product_image_zoom_size = '1024x1024' -%}
  {%- assign product_image_scale = '2' -%}
  {%- assign enable_zoom = section.settings.enable_zoom -%}

  {% case section.settings.image_size %}
  {% when 'small' %}
  {%- assign product_image_width = 'medium-up--one-third' -%}
  {%- assign product_description_width = 'medium-up--two-thirds' -%}
  {%- assign product_thumbnail_width = 'medium-up--one-third' -%}
  {%- assign height = 345 -%}
  {% when 'medium' %}
  {%- assign product_image_width = 'medium-up--one-half' -%}
  {%- assign product_description_width = 'medium-up--one-half' -%}
  {%- assign product_thumbnail_width = 'medium-up--one-quarter' -%}
  {%- assign height = 530 -%}
  {% when 'large' %}
  {%- assign product_image_width = 'medium-up--two-thirds' -%}
  {%- assign product_description_width = 'medium-up--one-third' -%}
  {%- assign product_thumbnail_width = 'medium-up--one-fifth' -%}
  {%- assign height = 720 -%}
  {% when 'full' %}
  {%- assign product_image_width = '' -%}
  {%- assign product_description_width = '' -%}
  {%- assign product_thumbnail_width = 'medium-up--one-eighth' -%}
  {%- assign height = 1090 -%}
  {%- assign enable_zoom = false -%}
  {% endcase %}  
  {% unless product.has_only_default_variant %}
  {% for option in product.options_with_values %}
  {% assign name = option.name | handleize %}
  {% if name == 'color' or name == 'colour' %}
  {% assign size_option = option.values | size %}
  {% if size_option > 1 %}
  {% for value in option.values %}
  {% assign default_color = option.selected_value %}               
  {% endfor %}
  {% endif %}
  {% endif %}
  {% endfor %}
  {% endunless %}
  <div class="grid product-single">
    <div class="grid__item product-single__photos {% if section.settings.image_size == 'full' %} product-single__photos--full{% endif %}">
      <span style="display:none;" class="template_suffix">{{ product.template_suffix }}</span>
      <div class="container_loader" style="display:none;">
      <img class="loader-img" src="{{'tenor.gif' | asset_url}}"/>
      </div>
      <div class="product-thumbnail">
        <div class="product-thumbnail-slider">
          {% for image in product.images %}
            {% if image.alt contains default_color %}
              <div class="sldier-list {% if image.alt contains 'youtube' or image.alt contains 'vimeo' or image.alt contains 'files' or image.alt contains 'mp4' %}sldier-list-video{% endif %}"><img class="product-single__thumbnail-image" src="{{ image.src | img_url: '200x200' , scale: 2}}" alt="{{ image.alt | escape }}"></div>
            {% endif %}
          {% endfor %}
        </div>
        {% comment %}{% unless product.metafields.video.thumb_video_image == blank %}
        <div class="sldier-list-youtube">
          <img class="product-single__thumbnail-image" src="{{ product.metafields.video.thumb_video_image }}" alt="{{ image.alt | escape }}">
        </div>
        {% endunless %}{% endcomment %}
      </div>
      <div class="product-mains">
        <div class="product-main-slider">
          {% for image in product.images %}
          	{% if image.alt contains default_color %}
              <div class="product-main-list">
                <div class="product-single__photo{% if enable_zoom %} js-zoom-enabled{% endif %}">

                  {% if image.alt contains "files" %}
                  {% assign urldata = image.alt | split: '=' | last %}
                  <div class="product-sigle-video-sec">
                   {% comment %} <iframe class="feature-row__image" src="{{ urldata }}" style="width: 100%; height: 100%;" frameborder="0" allowfullscreen=""></iframe> {% endcomment %}
                     <video class="feature-row__image" style="width: 100%; height: 100%;" autoplay muted loop playsinline><source src="{{ urldata }}" type="video/mp4"></video>
                  </div>

                  {% elsif image.alt contains 'mp4' %}
                  {% assign urldata = image.alt | split: '=' | last %}
                  <div class="product-sigle-video-sec">
                     <video autoplay muted loop playsinline class="feature-row__image " style="width: 100%; height: 100%;"><source src="{{ urldata }}" type="video/mp4"></video>
                  </div>

                  {% elsif image.alt contains 'vimeo' %}
                  {% assign urldata = image.alt | split: '=' | last %}
                  <div class="product-sigle-video-sec">
                    <iframe class="feature-row__image" src="{{ urldata }}" style="width: 100%; height: 100%;" ></iframe>
                  </div>
                  {% endif %}
                  <img class="feature-row__image" src="{{ image | img_url: '800x800' , scale: 2}}" alt="{{image.alt}}">
                </div>
              </div>
          	{% endif %}
          {% endfor %}
        </div>
        

        {% comment %}{% unless product.metafields.video.thumb_video == blank %}
        <div class="product-main-list-youtube">
          <div class="product-single__photo{% if enable_zoom %} js-zoom-enabled{% endif %}">
            <iframe class="feature-row__image" src="{{ product.metafields.video.thumb_video }}" style="width: 100%; height: 100%;"></iframe>
          </div>
        </div>
        {% endunless %}{% endcomment %}

        <div class="product-fanchybox-sec">
        {% assign count = 0 %}
          
          {% for image in product.images %}
          
          {% if image.alt contains default_color %}
           
          <div class="gallery-list gallery-list-{{ count }} ">
            <a class="fancybox" href="{{ image | img_url: '1600x1600', scale: 2 }}" data-fancybox="images" title="">
              <div class="gallery-list-section gallery-list-sec" style="background-image: url({{ image | img_url: '40x40' }}); width: 10px; height: 10px;">
              </div>
            </a>
          </div>
          {% assign count = count | plus: 1 %}
          {% endif %}
          {% endfor %}
        </div>
        <div id="gas-electric-hover" class="hide">
          <span class="custom-cursor-zoom"><img src="{{ 'zoom-icon-final.png' | asset_url }}"></span>
          <span class="custom-cursor-number"></span>
        </div>
        <div class="mob-thumbnail-slider">
          {% for image in product.images %}
          {% if image.alt contains default_color %}
          <div class="sldier-list"><img class="product-single__thumbnail-image" src="{{ image.src | img_url: '200x200' }}" alt="{{ image.alt | escape }}"></div>
          {% endif %}
          {% endfor %}
        </div>
      </div>
      {% comment %}
      <div class="product-mains">
        {%- assign featured_image = product.selected_or_first_available_variant.featured_image | default: product.featured_image -%}
        {% for image in product.images %}
        {% capture img_id %}FeaturedImage-{{ section.id }}-{{ image.id }}{% endcapture %}
        {% capture img_class %}product-featured-img{% endcapture %}
        {% capture zoom_img_id %}FeaturedImageZoom-{{ section.id }}-{{ image.id }}{% endcapture %}
        {% capture img_wrapper_id %}{{ zoom_img_id }}-wrapper{% endcapture %}
        {%- assign img_url = image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}

        {% include 'image-style' with small_style: true, width: height, height: height, wrapper_id: img_wrapper_id, img_id: img_id %}

        <div id="{{ img_wrapper_id }}" class="product-single__photo-wrapper js">
          <div id="{{ zoom_img_id }}" style="padding-top:{{ 1 | divided_by: image.aspect_ratio | times: 100}}%;" class="product-single__photo{% if enable_zoom %} js-zoom-enabled{% endif %}{% if product.images.size > 1 %} product-single__photo--has-thumbnails{% endif %}{% unless featured_image == image %} hide{% endunless %}" data-image-id="{{ image.id }}"{% if enable_zoom %} data-zoom="{{ image | img_url: product_image_zoom_size, scale: product_image_scale }}"{% endif %}>
            <img id="{{ img_id }}"
                 class="feature-row__image {{ img_class }} lazyload{% unless featured_image == image %} lazypreload{% endunless %}"
                 src="{{ image | img_url: '300x300' }}"
                 data-src="{{ img_url }}"
                 data-widths="[180, 360, 540, 720, 900, 1080, 1296, 1512, 1728, 2048]"
                 data-aspectratio="{{ image.aspect_ratio }}"
                 data-sizes="auto"
                 alt="{{ image.alt | escape }}">
          </div>
        </div>

        {% endfor %}

        <noscript>
          {% capture product_image_size %}{{ height }}x{% endcapture %}
          <img src="{{ featured_image | img_url: product_image_size, scale: product_image_scale }}" alt="{{ featured_image.alt }}" id="FeaturedImage-{{ section.id }}" class="product-featured-img" style="max-width: {{ height }}px;">
        </noscript>
      </div>

      {% if product.images.size > 1 %}
      {% if product.images.size > 3 %}
      {%- assign enable_thumbnail_slides = true -%}
      {% endif %}

      <div class="thumbnails-wrapper{% if enable_thumbnail_slides == true %} thumbnails-slider--active{% endif %}">
        {% if enable_thumbnail_slides == true %}
        <button type="button" class="btn btn--link medium-up--hide thumbnails-slider__btn thumbnails-slider__prev thumbnails-slider__prev--{{ section.id }}">
          {% include 'icon-chevron-left' %}
          <span class="icon__fallback-text">{{ 'sections.slideshow.previous_slide' | t }}</span>
        </button>
        {% endif %}
        <ul class="grid grid--uniform product-single__thumbnails product-single__thumbnails-{{ section.id }}">
          {% for image in product.images %}
          <li class="grid__item {{ product_thumbnail_width }} product-single__thumbnails-item js">
            <a href="{{ image.src | img_url: product_image_zoom_size, scale: product_image_scale }}"
               class="text-link product-single__thumbnail product-single__thumbnail--{{ section.id }}"
               data-thumbnail-id="{{ image.id }}"
               {% if enable_zoom %}data-zoom="{{ image.src | img_url: product_image_zoom_size, scale: product_image_scale }}"{% endif %}>
              <img class="product-single__thumbnail-image" src="{{ image.src | img_url: '110x110', scale: 2 }}" alt="{{ image.alt | escape }}">
            </a>
          </li>
          {% endfor %}
        </ul>
        {% if enable_thumbnail_slides == true %}
        <button type="button" class="btn btn--link medium-up--hide thumbnails-slider__btn thumbnails-slider__next thumbnails-slider__next--{{ section.id }}">
          {% include 'icon-chevron-right' %}
          <span class="icon__fallback-text">{{ 'sections.slideshow.next_slide' | t }}</span>
        </button>
        {% endif %}
      </div>
      {% endif %}
      {% endcomment %}
    </div>

    <div class="grid__item product-single-desription">
      <div class="product-single__meta product-single__meta-{{ settings.star_color }}">
        <div class="brand_name">
          {% assign handle = product.vendor | handleize %}
			{% comment %}
          <a href="{{ collections[handle].url }}">
            <h5>{{ product.vendor }}</h5>
          </a>
			{% endcomment %}
          {% if product.metafields.brand.brand_logo == blank %}
          {% capture brand %}brand-{{ product.vendor | handleize }}.png{% endcapture %}
          {% comment %}<img class="logo-nike" src='{{ brand | asset_url }}'>
          <a href="{{ collections[handle].url }}">
            <h5>{{ product.vendor }}</h5>
          </a>{% endcomment %}
          {% else %}

          {% comment %}<a href="{{ collections[handle].url }}">
          <img class="logo-nike" src="{{ product.metafields.brand.brand_logo }}">
          </a>{% endcomment %}

          {% endif %}
        </div>
        {% comment %}<img class="logo-nike" src="{{ 'nike-logo.png' | asset_url }}">{% endcomment %}
        <h1 itemprop="name" class="product-single__title main-txt-title">{{ product.title }}</h1>
        {% if section.settings.show_review %}
        {% include 'okendo-reviews-product-rating-summary' %}
        {% endif %}

        {% if section.settings.show_vendor %}
        <p itemprop="brand" class="product-single__vendor">{{ product.vendor }}</p>
        {% endif %}

        <div itemprop="offers" itemscope itemtype="http://schema.org/Offer">
          <meta itemprop="priceCurrency" content="{{ shop.currency }}">

          <link itemprop="availability" href="http://schema.org/{% if product.available %}InStock{% else %}OutOfStock{% endif %}">

          <p class="product-single__price product-single__price-{{ section.id }}{% unless current_variant.available %} product-price--sold-out{% endunless %}">
            {% if current_variant.compare_at_price > current_variant.price %}
            <span class="visually-hidden">{{ 'products.product.regular_price' | t }}</span>
            {% if product.price_max > product.price %} <span class="product-single__title main-txt-title">From </span> {% endif %}
            <s id="ComparePrice-{{ section.id }}"><span class="Bold-theme-hook-DO-NOT-DELETE bold_product_compare_price" data-override-value-set="1" data-override-value="{{ current_variant.compare_at_price }}" style="display:none !important;"></span>{{ current_variant.compare_at_price | money }}</s>
            <span class="product-price__price product-price__price-{{ section.id }} product-price__sale product-price__sale--single">
              <span id="ProductPrice-{{ section.id }}"
                    itemprop="price" content="{{ current_variant.price | divided_by: 100.00 }}" class="main-txt-title">
                <span class="Bold-theme-hook-DO-NOT-DELETE bold_product_price" data-override-value-set="1" data-override-value="{{ current_variant.price }}" style="display:none !important;"></span>{{ current_variant.price | money }}
              </span>
              {% comment %} <span class="product-price__sale-label product-price__sale-label-{{ section.id }}">{{ 'products.product.on_sale' | t }}</span>  {% endcomment %}
            </span>
            {% else %}
            <span class="visually-hidden">{{ 'products.product.regular_price' | t }}</span>
            {% if product.price_max > product.price %} <span class="product-single__title main-txt-title">From </span> {% endif %}
            <s id="ComparePrice-{{ section.id }}" class="hide"><span class="Bold-theme-hook-DO-NOT-DELETE bold_product_compare_price_2" data-override-value-set="1" data-override-value="{{ current_variant.compare_at_price }}" style="display:none !important;"></span>{{ current_variant.compare_at_price | money }}</s>
            <span class="product-price__price product-price__price-{{ section.id }}">

              <span id="ProductPrice-{{ section.id }}"
                    itemprop="price" content="{{ current_variant.price | divided_by: 100.00 }}" class="main-txt-title">
                <span class="Bold-theme-hook-DO-NOT-DELETE bold_product_price_2" data-override-value-set="1" data-override-value="{{ current_variant.price }}" style="display:none !important;"></span> {{ product.price_min | money }}
              </span>
              {% comment %}<span class="product-price__sale-label product-price__sale-label-{{ section.id }} hide">{{ 'products.product.on_sale' | t }}</span> {% endcomment %}
            </span>
            {% endif %}


          </p>
          {% if settings.product_sticker_status %}
          
          <div class="free-icons">
            <ul class="shipping_methods" >
              {% for block in section.blocks %}
              {% if block.type == 'shipping_methods' %}
              {% assign collectionh = block.settings.collection %}
              {% if collectionids contains collections[collectionh].id %}

              {% if sticker_flag == 0 %}
                {% if freesheep != '' and free_exp_sheep != '' %}
                  {% assign sticker_flag = 1 %}
             	 	{% if settings.freeexpress or settings.freeexpress_first !='' or settings.freeexpress_last !='' %}
                      <li class="free-express-shipping {% if settings.freeexpress %} {% else %} freebg  {% endif %}">
                      {% if settings.freeexpress %}
                      <img src="{{ settings.freeexpress | img_url: '150x150' }}">
                        {% else %}
                       <span>{{settings.freeexpress_first}}</span>
                       <span>{{settings.freeexpress_last}}</span>
                      {% endif %}
                      </li>
                	 {% endif %}
                {% else %}
                  {% if collections[collectionh].id == coll_free_exp_ship %}
              	 {% if settings.freeexpress or settings.freeexpress_first !='' or settings.freeexpress_last !='' %}
                      <li class="free-express-shipping {% if settings.freeexpress %} {% else %} freebg  {% endif %}">
                      {% if settings.freeexpress %}
                      <img src="{{ settings.freeexpress | img_url: '150x150' }}">
                        {% else %}
                   <span>{{settings.freeexpress_first}}</span>
                   <span>{{settings.freeexpress_last}}</span>
                         
                      {% endif %}
                    {% endif %}
                      </li>
                  {% endif %}


              {% if coll_freeship != "" %}
                  {% if collections[collectionh].id == coll_freeship %}
                        {% if settings.freeshiping or settings.freeshiping_first !='' or settings.freeshiping_last !='' %}
                              <li class="freeshipping {% if settings.freeshiping %} {% else %} freebg  {% endif %}">
                              {% if settings.freeshiping %}

                              <img src="{{ settings.freeshiping | img_url: '150x150' }}">
                                {% else %}
                   <span>{{settings.freeshiping_first}}</span>
                   <span>{{settings.freeshiping_last}}</span>

                              {% endif %}
                              </li>
                       {% endif %}

                  {% endif %}
                {% endif %}
              {% endif %}
               {% endif %}


			{% if in_hats_collection %}
               {% if block.settings.shipping_method == 'return_on_shoes' %}
              	{% if settings.returnonshoes or settings.returnonshoes_first !='' or settings.returnonshoes_last !='' %}
                 <li class="return_on_shoes {% if settings.returnonshoes %} {% else %} freebg  {% endif %}">
                 {% if settings.returnonshoes %}
                 <img src="{{ settings.returnonshoes | img_url: '150x150' }}">
                   {% else %}
                   <span>{{settings.returnonshoes_first}}</span>
                   <span>{{settings.returnonshoes_last}}</span>
                 {% endif %}
                 </li>
               {% endif %}
              {% endif %}
  			{% endif %}

			  {% if heavy_bulky != "" %}
              {% if collections[collectionh].id == heavy_bulky %}
              {% if settings.blikyitems or settings.blikyitems_first !='' or settings.blikyitems_last !='' %}
              <li class="blikyitems {% if settings.blikyitems %} {% else %} freebg  {% endif %}">
              {% if settings.blikyitems %}
              <img src="{{ settings.blikyitems | img_url: '150x150' }}">
                {% else %}
                   <span>{{settings.blikyitems_first}}</span>
                   <span>{{settings.blikyitems_last}}</span>
              {% endif %}
              </li>
              {% endif %}
              {% assign isBulky = 1 %}
              {% endif %}
              {% endif %}

			{% comment %}
			  {% if three_ships_free != "" %}
              {% if collections[collectionh].id == three_ships_free %}
              {% if settings.threeships or settings.threeships_first !='' or settings.threeships_last !='' %}
              <li class="3-Ships-Free {% if settings.threeships %} {% else %} freebg  {% endif %}">
              {% if settings.threeships %}
              <img src="{{ settings.threeships | img_url: '150x150' }}">
                {% else %}
                   <span>{{settings.threeships_first}}</span>
                   <span>{{settings.threeships_last}}</span>
              {% endif %}
              </li>
              {% endif %}
              {% endif %}
              {% endif %}
		{% endcomment %}
              
			  {% if ships_separatly != "" %}
              {% if collections[collectionh].id == ships_separatly %}
              {% if settings.separately or settings.separately_first !='' or settings.separately_last !='' %}
              <li class="ships-separately {% if settings.separately %} {% else %} freebg  {% endif %}">
              {% if settings.separately %}
              <img src="{{ settings.separately | img_url: '150x150' }}">
                 {% else %}
                   <span>{{settings.separately_first}}</span>
                   <span>{{settings.separately_last}}</span>
              {% endif %}
              </li>
              {% endif %}
              {% endif %}
              {% endif %}

              {% if block.settings.shipping_method == 'same_day_shipping' and prtag != '' %}
               {% if settings.separately or settings.separately_first !='' or settings.separately_last !='' %}
              <li class="ships-separately {% if settings.separately %} {% else %} freebg  {% endif %}">
              {% if settings.separately %}
              <img src="{{ settings.separately | img_url: '150x150' }}">
                {% else %}
                   <span>{{settings.separately_first}}</span>
                   <span>{{settings.separately_last}}</span>
              {% endif %}
              </li>
               {% endif %}
              {% endif %}

              {% endif %}

              {% endif %}
              {% endfor %}
            </ul>
          </div>

		{% endif %}
          <div class="clearfix"></div>
		  <!-- Zip WIDGET -->
          {% include 'product-afterpay' %}
 		  {% comment %}{%include 'zip-widget'%}{% endcomment %}
          <div data-zm-merchant="25673dd4-b934-482b-a8a3-53ef04dc7136" data-env="production"></div>
                  
          <style>
            .shopify-zip-prod-widget > iframe {
              height: 60px!important;
              max-width: min-content!important;
              margin-bottom: -20px;
            }
                .a-size-chart-lightbox p .size-desc{display: inline-block;
    padding: 0;     position: relative;
    top: -1px;
            line-height: 1;}
          </style>
          <span style="cursor:pointer" class="shopify-zip-prod-widget" data-zm-asset="productwidget" data-zm-widget="popup" data-zm-popup-asset="termsdialog"></span>
          <!-- end Zip WIDGET -->
          {% include 'shipping-countdown' %}
			<div class="clearfix"></div>
          <form action="/cart/add" method="post" enctype="multipart/form-data" class="product-form product-form-{{ section.id }}{% unless section.settings.show_variant_labels %} product-form--hide-variant-labels{% endunless %}" data-section="{{ section.id }}">

            {% for block in section.blocks %}
            {% if block.type == 'shipping_methods' %}

            {% assign collectionh = block.settings.collection %}
            {% if collectionids contains collections[collectionh].id %}
            
            <!-- Start Free / free exp shiping -->
            {% if block.settings.shipping_method == 'free_express_shipping_au_and_nz' %}
