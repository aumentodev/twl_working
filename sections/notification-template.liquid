<script>
$(".new-size ul").on("click", ".init", function() {
    $(this).closest(".new-size ul").children('.new-size ul li:not(.init)').toggleClass('size-view');
});

var allOptions = $(".new-size ul").children('.new-size ul li:not(.init)');
$(".new-size ul").on("click", "li.available", function() {
    allOptions.removeClass('selected');
    $(this).addClass('selected');
    $(".new-size ul").children('.init').html($(this).html());
    allOptions.toggleClass('size-view');
});
</script>
	<script>
		jQuery(document).ready(function (e) {
    function t(t) {
        e(t).bind("click", function (t) {
            t.preventDefault();
            e(this).parent().fadeOut()
        })
    }
    e(".dropdown-toggle").click(function () {
        var t = e(this).parents(".button-dropdown").children(".dropdown-menu-size").is(":hidden");
        e(".button-dropdown .dropdown-menu-size").hide();
        e(".button-dropdown .dropdown-toggle").removeClass("active");
        if (t) {
            e(this).parents(".button-dropdown").children(".dropdown-menu-size").toggle().parents(".button-dropdown").children(".dropdown-toggle").addClass("active")
        }
    });
    e(document).bind("click", function (t) {
        var n = e(t.target);
        if (!n.parents().hasClass("button-dropdown")) e(".button-dropdown .dropdown-menu-size").hide();
    });
    e(document).bind("click", function (t) {
        var n = e(t.target);
        if (!n.parents().hasClass("button-dropdown")) e(".button-dropdown .dropdown-toggle").removeClass("active");
    })
});

</script>
